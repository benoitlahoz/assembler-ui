<% if (typeof files !== 'undefined' && Array.isArray(files)) { %>
<% files.forEach(function(file) { %>

## <%= file.title || file.name %>
::hr-underline
::

<% if (file.description) { %>
<%= file.description %>
<% } %>

<% if (file.api) { %>
**API**: <%= file.api %>
<% } %>

<% if (file.author) { %>
<% if (Array.isArray(file.author)) { %>
**Authors**:<br>
<% file.author.forEach(function(author) { %>
<%= author %><br>
<% }); %>
<% } else { %>
**Author**: <%= file.author %>
<% } %>
<% } %>

<% if (file.doc && file.doc.variants && Object.keys(file.doc.variants).length) { %>
  <%- include('../sections/variants-section.ejs', { variants: file.doc.variants }) %>
<% } %>

<% if (file.doc && file.doc.props && file.doc.props.length) { %>
  <%- include('../sections/props-section.ejs', { props: file.doc.props }) %>
<% } %>

<% if (file.doc && file.doc.emits && file.doc.emits.length) { %>
  <%- include('../sections/events-section.ejs', { events: file.doc.emits }) %>
<% } %>

<% if (file.doc && file.doc.slots && file.doc.slots.length) { %>
  <%- include('../sections/slots-section.ejs', { slots: file.doc.slots }) %>
<% } %>

<% if (file.doc && file.doc.provides && file.doc.provides.length) { %>
  <%- include('../sections/provides-section.ejs', { provides: file.doc.provides }) %>
<% } %>

<% if (file.doc && file.doc.injects && file.doc.injects.length) { %>
  <%- include('../sections/injects-section.ejs', { injects: file.doc.injects }) %>
<% } %>

<% if (file.doc && file.doc.exposes && file.doc.exposes.length) { %>
  <%- include('../sections/exposes-section.ejs', { exposes: file.doc.exposes }) %>
<% } %>

<% if (file.doc && file.doc.cssVars && file.doc.cssVars.length) { %>
  <%- include('../sections/cssvars-section.ejs', { cssVars: file.doc.cssVars }) %>
<% } %>

<% if (file.doc && file.doc.childComponents && file.doc.childComponents.length) { %>
  <%- include('../sections/childcomponents-section.ejs', { childComponents: file.doc.childComponents }) %>
<% } %>

<% }) %>
<% } %>

